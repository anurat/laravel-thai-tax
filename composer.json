{
    "name": "connectiv/thai_tax",
    "description": "Thai tax calculation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anurat Chapanond",
            "email": "anurat@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Connectiv\\ThaiTax\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Connectiv\\ThaiTax\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Connectiv\\ThaiTax\\ThaiTaxServiceProvider"
            ],
            "aliases": {
                "ThaiTax": "Connectiv\\ThaiTax\\Facades\\ThaiTax"
            }
        }
    },
    "require": {
        "php": ">=7",
        "laravel/framework": ">=7"
    },
    "require-dev": {
        "orchestra/testbench": "6.0",
        "phpunit/phpunit": "^9.5"
    }
}
